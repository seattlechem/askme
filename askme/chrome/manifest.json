{
  "name": "Anna personal helper",
  "version": "1.0",
  "description": "Virtual brain",
  "permissions": ["tabs", "tabCapture", "downloads", "storage"],
  "background": {
    "scripts": ["popup.js"],
    "persistent": true
  },
  "browser_action": {
    "default_icon": "images/get_started48.png",
    "default_popup": "popup.html",
    "default_title": "Open Chrome Audio Capture interface"
  },
  "options_page": "options.html",
  "manifest_version": 2
}
